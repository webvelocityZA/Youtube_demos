  "key_features": {
    "authentication": {
      "description": "Secure member authentication with multi-factor options",
      "components": ["AuthGuard", "AuthInterceptorService"]
    },
    "responsive_design": {
      "description": "Mobile-responsive design with Angular Material components",
      "frameworks": ["Angular Material", "Angular CDK Layout"]
    },
    "guided_tours": {
      "description": "Interactive guided tours for new users",
      "library": "ngx-guided-tour"
    },
    "session_management": {
      "description": "Automatic session timeout and keepalive functionality",
      "library": "@ng-idle/keepalive"
    },
    "maps_integration": {
      "description": "Google Maps integration for provider search and locations",
      "library": "@angular/google-maps"
    },
    "pagination": {
      "description": "Paginated content display for large datasets",
      "library": "ngx-pagination"
    }
