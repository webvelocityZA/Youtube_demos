{
  "application": {
    "name": "GEMS Member Portal",
    "description": "Comprehensive member portal for GEMS medical scheme members to access benefits, manage profiles, and handle healthcare-related tasks",
    "framework": "Angular",
    "authentication": "Required for most pages (AuthGuard protected)"
  },
  "pages": {
    "auth": {
      "login": {
        "title": "Login",
        "url": "/Login",
        "description": "Member authentication page with username/password login",
        "actions": ["Login", "Register", "Reset Password", "Retrieve Username"],
        "components": ["LoginComponent"],
        "access": "public"
      },
      "register": {
        "title": "Registration",
        "url": "/register",
        "description": "New member registration and account creation",
        "actions": ["Create Account", "Submit Registration"],
        "components": ["RegisterComponent"],
        "access": "public"
      },
      "reset_password": {
        "title": "Reset Password",
        "url": "/reset-password",
        "description": "Password reset functionality for existing members",
        "actions": ["Reset Password", "Submit Reset Request"],
        "components": ["ResetPasswordComponent"],
        "access": "public"
      },
      "retrieve_username": {
        "title": "Retrieve Username",
        "url": "/retrieve-username",
        "description": "Username recovery for members who forgot their login credentials",
        "actions": ["Retrieve Username", "Submit Request"],
        "components": ["RetrieveUsernameComponent"],
        "access": "public"
      }
